{
  "name": "patientportal",
  "version": "1.0.0",
  "description": "Code challenge app for Done",
  "license": "MIT",
  "scripts": {
    "start:portal": "parcel portal/index.html --dist-dir ./portal/dist",
    "build:portal": "parcel build portal/index.html --dist-dir ./.server_dist/public",
    "start:server": "ts-node-dev --log-error server/index.ts",
    "build:server": "tsc --build server/tsconfig.json",
    "start:production": "yarn build:server && yarn build:portal && node ./.server_dist/index.js",
    "db:setup": "psql -f server/db/migrations/setup.sql",
    "db:clean": "psql -f server/db/migrations/cleanup.sql",
    "db:seed": "./node_modules/.bin/ts-node server/db/seed.ts",
    "makeenv": "cp .env.template .env"
  },
  "devDependencies": {
    "@parcel/transformer-sass": "2.6.1",
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.4",
    "@types/morgan": "^1.9.3",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5",
    "parcel": "^2.6.1",
    "process": "^0.11.10",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@apollo/client": "^3.6.9",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-graphql": "^0.12.0",
    "express-session": "^1.17.3",
    "formik": "^2.2.9",
    "graphql": "^16.5.0",
    "morgan": "^1.10.0",
    "node-fetch": "^3.2.6",
    "pg-promise": "^10.11.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-google-autocomplete": "^2.7.0",
    "ts-node-dev": "^2.0.0",
    "yup": "^0.32.11"
  }
}
